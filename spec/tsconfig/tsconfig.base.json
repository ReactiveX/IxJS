{
//   "extends": "../../tsconfig.json",
  "extends": "../../tsconfig/tsconfig.base.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "importHelpers": false,
    "noEmitHelpers": false,
    "noEmitOnError": false,
    "sourceMap": false,
    "inlineSources": false,
    "inlineSourceMap": false,
    "downlevelIteration": false,
    "baseUrl": "../../",
    "paths": {
      "ix": ["src/Ix.node"],
      "ix/*": ["src/*"]
    }
  }
}
