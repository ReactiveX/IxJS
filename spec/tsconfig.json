{
  "extends": "../tsconfig.json",
  "include": [
    "../src/**/*.ts",
    "../spec/**/*.ts"
  ],
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "CommonJS",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "importHelpers": false,
    "noEmitHelpers": false,
    "noEmitOnError": false,
    "sourceMap": false,
    "inlineSources": false,
    "inlineSourceMap": false,
    "downlevelIteration": false,
    "baseUrl": "../",
    "paths": {
      "ix/*": [
        "src/*"
      ]
    }
  }
}
