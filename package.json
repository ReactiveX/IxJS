{
  "name": "ix",
  "version": "2.0.0",
  "main": "./targets/es5/cjs/Ix.js",
  "module": "./targets/es5/esm/Ix.js",
  "browser": "./targets/es5/umd/Ix.js",
  "jsnext:main": "./targets/es2015/esm/Ix.js",
  "description": "The Interactive Extensions for JavaScript",
  "scripts": {
    "lerna": "lerna",
    "commit": "git-cz",
    "test": "gulp test",
    "build": "gulp build",
    "clean": "gulp clean",
    "validate": "npm-run-all lint build test",
    "lerna:publish": "lerna publish --conventional-commits",
    "prepublishOnly": "npm-run-all validate doc lerna:publish",
    "commitmsg": "validate-commit-msg",
    "doc": "shx rm -rf ./doc && esdoc",
    "lint": "npm-run-all -p lint:*",
    "lint:src": "tslint --fix --type-check -p tsconfig.json -c tslint.json \"src/**/*.ts\"",
    "lint:spec": "tslint --fix --type-check -p spec/tsconfig.json -c tslint.json \"spec/**/*.ts\""
  },
  "author": "Matthew Podwysocki <matthewp@microsoft.com>",
  "homepage": "https://github.com/ReactiveX/IxJS#readme",
  "bugs": {
    "url": "https://github.com/ReactiveX/IxJS/issues"
  },
  "license": "MIT",
  "keywords": [
    "Iterator",
    "Iterable",
    "Promise",
    "Async",
    "AsyncIterable",
    "AsyncIterator"
  ],
  "files": [
    "dist",
    "types",
    "targets",
    "LICENSE",
    "readme.md"
  ],
  "peerDependencies": {
    "tslib": "^1.7.1"
  },
  "typings": "./types/Ix.d.ts",
  "devDependencies": {
    "@types/node": "~8.0.10",
    "@types/tape": "~4.2.30",
    "command-line-args": "~4.0.6",
    "commitizen": "~2.9.6",
    "cz-conventional-changelog": "~2.0.0",
    "del": "~3.0.0",
    "esdoc": "~0.5.2",
    "google-closure-compiler": "~20170626.0.0",
    "gulp": "~3.9.1",
    "gulp-json-transform": "~0.4.2",
    "gulp-sourcemaps": "~2.6.0",
    "gulp-typescript": "~3.2.0",
    "husky": "~0.14.3",
    "lerna": "^2.0.0",
    "lint-staged": "~4.0.1",
    "merge2": "~1.1.0",
    "mkdirp": "~0.5.1",
    "npm-run-all": "~4.0.2",
    "pump": "~1.0.2",
    "rimraf": "~2.6.1",
    "shx": "~0.2.2",
    "tap-difflet": "~0.7.0",
    "tape": "~4.7.0",
    "ts-node": "~3.2.0",
    "tsickle": "~0.23.3",
    "tslib": "~1.7.1",
    "tslint": "~5.5.0",
    "tsutils": "~2.6.0",
    "typescript": "~2.4.1",
    "validate-commit-msg": "~2.12.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.@(ts)": [
      "tslint --fix",
      "git add"
    ]
  },
  "esdoc": {
    "source": "./targets/es2015/esm",
    "destination": "./doc",
    "test": {
      "type": "tape",
      "source": "./spec"
    },
    "experimentalProposal": {
      "asyncGenerators": true
    }
  }
}
